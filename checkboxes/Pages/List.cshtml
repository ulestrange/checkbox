@page
@model checkboxes.Pages.ListModel
@{
}


<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Colours List</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var decor in Model.Decors)
        {
            // list out each of the properties.
            <tr>
                <td>@decor.ID</td>
                <td>

                    @decor.Colours
                </td>

                <td> <a asp-page="Update" asp-route-id="@decor.ID">Change</a> </td>
            </tr>
        }
    </tbody>
</table>
